@model IEnumerable<PWA_Test.Models.Comparable>
@{
    ViewBag.Title = "Comparables List";
}
<div>
    <h4>Comparables List</h4>
    <hr />
    @foreach (var item in Model)
    {
        <dl class="dl-horizontal">
            <dt>Rank @item.rank property</dt>
            <dd>
                @Html.ActionLink(" ", "Details", new { rank=item.rank }, new { @class="link-icon" } )
            </dd>

            @*<dt>
                @Html.DisplayFor(model => item.postcode)
            </dt>*@
        </dl>
    }
</div>
